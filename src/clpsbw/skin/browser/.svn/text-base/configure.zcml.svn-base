<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="affinitic.skin">


<!-- nouzotre portal Zope 3 browser layer -->
  <interface
      interface=".interfaces.IAffiniticTheme"
      type="zope.publisher.interfaces.browser.IBrowserSkinType"
      name="Affinitic skin layer"
      />


<!-- ressources -->

  <browser:resource
      file="js/supersized.3.2.7.min.js"
      name="supersized.3.2.7.min.js"
      />

  <browser:resource
      file="js/supersized.shutter.min.js"
      name="supersized.shutter.min.js"
      />

  <browser:resource
      file="js/jquery.easing.min.js"
      name="jquery.easing.min.js"
      />

  <browser:resource
      file="js/affinitic.js"
      name="affinitic.js"
      />


</configure>
