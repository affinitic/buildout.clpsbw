<?xml version="1.0"?>
<object name="portal_skins" 
        allow_any="False" 
        cookie_persistence="False"
        default_skin="clpsbw skin">

 <object name="clpsbw_images"
         meta_type="Filesystem Directory View"
         directory="clpsbw.skin:skins/clpsbw_images"/>
 <object name="clpsbw_templates"
         meta_type="Filesystem Directory View"
         directory="clpsbw.skin:skins/clpsbw_templates"/>
 <object name="clpsbw_styles"
         meta_type="Filesystem Directory View"
         directory="clpsbw.skin:skins/clpsbw_styles"/>

 <skin-path name="clpsbw skin" 
            based-on="Sunburst Theme">
  <layer name="clpsbw_images"
         insert-after="custom"/>
  <layer name="clpsbw_templates"
         insert-after="clpsbw_images"/>
  <layer name="clpsbw_styles"
         insert-after="clpsbw_templates"/>
 </skin-path>

</object>
